{
  "name": "sapic-ne",
  "version": "0.1.0",
  "author": "Tim <truecarry@gmail.com>",
  "description": "Steam.Design Desktop",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sapic/sapic-ne.git"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder --publish=never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps"
  },
  "build": {
    "productName": "Steam.Design",
    "appId": "design.steam",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "lib/greenworks/lib/",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@tweenjs/tween.js": "^18.5.0",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "discord-rpc": "^3.1.0",
    "electron-updater": "^4.2.0",
    "fs": "0.0.2",
    "jimp": "^0.9.3",
    "path": "^0.12.7",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-register": "^6.26.0",
    "cfonts": "^2.4.5",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^7.1.6",
    "electron-builder": "22.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^4.4.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.1",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.8.2",
    "mocha": "^6.2.2",
    "multispinner": "^0.2.1",
    "native-ext-loader": "^2.3.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "register-scheme": "0.0.2",
    "require-dir": "^1.2.0",
    "spectron": "^9.0.0",
    "style-loader": "^1.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "url-loader": "^2.3.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.3",
    "webpack-dev-server": "^3.10.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
